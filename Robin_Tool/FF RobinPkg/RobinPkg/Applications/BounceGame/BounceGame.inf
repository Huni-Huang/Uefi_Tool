  Graphic.c
  Graphic.h 
  Window.c 
  Window.h 

[Packages]
  StdLib/StdLib.dec
  MdePkg/MdePkg.dec
  MdeModulePkg/MdeModulePkg.dec
  ShellPkg/ShellPkg.dec

[LibraryClasses]
  LibC
  LibStdio
  DevShell

  UefiLib
  ShellCEntryLib
  FileHandleLib
