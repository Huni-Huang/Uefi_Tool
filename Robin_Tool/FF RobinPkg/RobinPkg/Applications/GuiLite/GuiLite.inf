  Common.c
  Common.h
  Graphic.c
  Graphic.h 
  Window.c
  Window.h
[Packages]
  StdLib/StdLib.dec
  MdePkg/MdePkg.dec
  ShellPkg/ShellPkg.dec

[LibraryClasses]
  LibC
  LibStdio
  DevShell
  LibMath  #for cos sin...

  BaseMemoryLib
  ShellLib
  ShellCEntryLib
  UefiBootServicesTableLib
